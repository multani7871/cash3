{
  "name": "functions",
  "description": "Cloud Functions for Firebase",
  "engines": {
    "node": "8"
  },
  "scripts": {
    "lint": "eslint .",
    "serve": "./node_modules/firebase-tools/bin/firebase serve --only functions",
    "shell": "./node_modules/firebase-tools/bin/firebase functions:shell",
    "start": "npm run shell",
    "getEnvKey": "curl -s https://raw.githubusercontent.com/envkey/envkey-source/master/install.sh | bash",
    "deploy": "eval $(envkey-source) && ./node_modules/firebase-tools/bin/firebase deploy --only functions --token $$",
    "logs": "./node_modules/firebase-tools/bin/firebase functions:log"
  },
  "dependencies": {
    "cors": "^2.8.4",
    "envkey": "^1.1.2",
    "firebase": "^5.3.1",
    "firebase-admin": "latest",
    "firebase-functions": "latest",
    "googleapis": "^32.0.0",
    "plaid": "latest",
    "firebase-tools": "latest"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.10.0"
  },
  "private": true
}
