{
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "functions": {
  },
  "hosting": {
    "predeploy": [
      "npm run build"
    ],
    "public": "build",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/createCalendar",
        "function": "createCalendar"
      },
      {
        "source": "/deleteCalendar",
        "function": "deleteCalendar"
      },
      {
        "source": "/exchangePublicToken",
        "function": "exchangePublicToken"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
